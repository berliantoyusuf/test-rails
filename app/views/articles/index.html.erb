<h1>Articles</h1>

<ul>
  <% @articles.each_with_index do |article, index| %>
    <li>
      <a href="<%= article_path(article) %>"> Article <%= index + 1 %> </a>
    </li>
    <div> Title: <%= link_to article.title, article %> </div>
    <div> Body: <%= article.body %> </div>
    <div> </div>
    <% end %>
</ul>

<%= link_to "New Article", new_article_path %>